  "bank_categories": {
    "CHIPS_PARTICIPANT": {
      "description": "Large banks (~47) that are CHIPS participants. All are automatically FED-enabled.",
      "characteristics": [
        "Direct CHIPS access",
        "Direct FED settlement account",
        "Can route via CHIPS (cheaper) or FED (faster)",
        "No correspondent bank needed"
      ],
      "examples": ["JPMorgan Chase", "Wells Fargo", "Bank of America", "Citibank"]
    },
    "FED_ENABLED": {
      "description": "Regional and mid-size banks that are FED-enabled but not CHIPS participants.",
      "characteristics": [
        "Direct FED settlement account",
        "No CHIPS access",
        "Route via FED only",
        "No correspondent bank needed"
      ],
      "examples": ["U.S. Bank", "PNC Bank", "Truist Bank"]
    },
    "NON_FED_ENABLED": {
      "description": "Small banks, credit unions, and AUs that do not have FED settlement account license.",
      "characteristics": [
        "No direct FED settlement account",
        "No CHIPS access",
        "Require correspondent bank (FED-enabled) for settlement",
        "Route via FED using correspondent's settlement account"
      ],
      "examples": ["Idaho Central Credit Union", "South Carolina Federal Credit Union", "Navy Federal Credit Union"]
    }
  },
  "routing_rules": {
    "chips_participant": {
      "fed_enabled": true,
      "chips_enabled": true,
      "requires_correspondent": false,
      "routing_options": ["CHIPS", "FED"],
      "default_network": "CHIPS",
      "fallback_network": "FED"
    },
    "fed_enabled_non_chips": {
      "fed_enabled": true,
      "chips_enabled": false,
      "requires_correspondent": false,
      "routing_options": ["FED"],
      "default_network": "FED"
    },
    "non_fed_enabled": {
      "fed_enabled": false,
